@model IEnumerable<db_1.Models.AsignaturasEstudiante>

@{
    ViewData["Title"] = "Index";
}

<center><h1>Lista de Asignaturas de Estudiantes</h1></center>

<p>
    <center><a asp-action="Create" class="btn btn-warning">Registrar Asignaturas de Estudiantes</a></center>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Fecha Registro
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Asignatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                Fecha Registro
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Asignatura.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estudiante.Id)
            </td>
            <td>
                <a asp-action="Edit" class="btn btn-success" asp-route-id="@item.Id">Actualizar</a> |
                <a asp-action="Details" class="btn btn-dark" asp-route-id="@item.Id">Ver</a> |
                <a asp-action="Delete" class="btn btn-danger" asp-route-id="@item.Id">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
